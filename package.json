{
  "name": "@toyz/loom",
  "version": "0.11.2",
  "type": "module",
  "description": "Decorator-driven web component framework with reactive state, DOM morphing, DI, and JSX",
  "license": "MIT",
  "author": "toyz",
  "repository": {
    "type": "git",
    "url": "https://github.com/Toyz/loom.git"
  },
  "homepage": "https://toyz.github.io/loom/",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js"
    },
    "./router": {
      "types": "./dist/router/index.d.ts",
      "import": "./dist/router/index.js"
    },
    "./store": {
      "types": "./dist/store/index.d.ts",
      "import": "./dist/store/index.js"
    },
    "./di": {
      "types": "./dist/di/index.d.ts",
      "import": "./dist/di/index.js"
    },
    "./element": {
      "types": "./dist/element/index.d.ts",
      "import": "./dist/element/index.js"
    },
    "./transform": {
      "types": "./dist/transform/index.d.ts",
      "import": "./dist/transform/index.js"
    },
    "./element/icon": {
      "types": "./dist/element/icon.d.ts",
      "import": "./dist/element/icon.js"
    },
    "./element/virtual": {
      "types": "./dist/element/virtual.d.ts",
      "import": "./dist/element/virtual.js"
    },
    "./element/canvas": {
      "types": "./dist/element/canvas.d.ts",
      "import": "./dist/element/canvas.js"
    },
    "./element/image": {
      "types": "./dist/element/image.d.ts",
      "import": "./dist/element/image.js"
    },
    "./decorators": {
      "types": "./dist/decorators/index.d.ts",
      "import": "./dist/decorators/index.js"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "import": "./dist/testing.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm test && npm run build"
  },
  "devDependencies": {
    "happy-dom": "^20.6.1",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "web-components",
    "decorators",
    "reactive",
    "jsx",
    "dom-morphing",
    "shadow-dom",
    "dependency-injection"
  ]
}
